<body class="Home">
    <main class="Login-section">
    <p class="Home-welcome">Welcome to the cafeteria management system</p>
    <div>
        <h2 class="Login-title">User Authentication</h2>
        <%= form_with(url: "/todos", method: "post", class: "Login-form") do %>
            <p >Login ID :<%= text_field(nil, nil, placeholder: "Enter your ID here", class:"Login-details") %></p>
            <p>Password :<%= password_field(nil, nil, placeholder: "Enter your password here", class:"Login-details") %></p>
            <p><%= submit_tag(:SignIn, class: "SignIn-button") %>
            <%= button_tag(:SignUp, onclick: "window.location.href = 'https://w3docs.com';", class: "SignUp-button") %><p>
        <% end %>
    </div>
    </main>
</body>
